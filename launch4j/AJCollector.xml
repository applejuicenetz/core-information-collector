<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
    <dontWrapJar>true</dontWrapJar>
    <headerType>gui</headerType>
    <jar>AJCollector.jar</jar>
    <outfile>AJCollector.exe</outfile>
    <chdir>.</chdir>
    <priority>normal</priority>
    <downloadUrl>https://www.java.com/de/download/manual.jsp</downloadUrl>
    <supportUrl>https://applejuicenet.cc/</supportUrl>
    <stayAlive>false</stayAlive>
    <restartOnCrash>false</restartOnCrash>
    <icon>../assets/windows/AJCollector.ico</icon>
    <singleInstance>
        <mutexName>AJCollector</mutexName>
    </singleInstance>
    <jre>
        <path>Java;runtime;%JAVA_HOME%;%PATH%</path>
        <bundledJreAsFallback>false</bundledJreAsFallback>
        <minVersion>21</minVersion>
        <jdkPreference>preferJre</jdkPreference>
        <requiresJdk>false</requiresJdk>
    </jre>
</launch4jConfig>
